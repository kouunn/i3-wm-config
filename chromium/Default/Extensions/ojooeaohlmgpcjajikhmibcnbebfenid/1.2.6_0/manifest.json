{
   "author": "DorsyWang(王斌)",
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon/48.png"
   },
   "content_scripts": [ {
      "js": [ "google-analytics-bundle.js", "alloydesigner.js" ],
      "matches": [ "*://*/*", "file:///*" ]
   } ],
   "default_locale": "en",
   "description": "A web front-end development tool,前端重构开发辅助工具",
   "homepage_url": "http://alloyteam.github.io/AlloyDesigner/",
   "icons": {
      "128": "icon/128.png",
      "16": "icon/16.png",
      "48": "icon/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "AlloyDesigner",
   "permissions": [ "https://www.google-analytics.com/", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.2.6"
}
